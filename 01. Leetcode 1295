// Find Numbers with Even Number of Digits
// Leetcode - 1295

class Solution {
public:
    int findNumbers(vector<int>& nums) {
        vector<string>str;
        for(auto x:nums){
            str.push_back(to_string(x));
        }

        int count = 0;
        for(auto x:str){
            if(x.size()%2==0) count++;
        }
        return count;
    }
};
