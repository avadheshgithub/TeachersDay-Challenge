// Move Zeros
// leetcode - 283


Methode 01 - Two Pointers

class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int n = nums.size();
        int k = 0;
        for(int i=0;i<n;i++){
            if(nums[i]!=0){
                nums[k]=nums[i];
                k++;
            }
        }

        for(int i=k;i<n;i++){
            nums[i]=0;
        }
    }
};

-------------------------------------------------

Methode 02 - using Zero's Count

class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int n = nums.size();
       int noz=0;
        vector<int>ans;
        for(int i=0;i<n;i++){
            if(nums[i]==0) noz++;
            else ans.push_back(nums[i]);
        }

        for(int i=0;i<n;i++){
            if(i<n-noz) nums[i] = ans[i];
            else nums[i]=0;
        }
    }
};
